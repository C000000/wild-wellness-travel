<!DOCTYPE html>
<html>
  <head>
    <title>Wild Wellness Travel</title>
    <%= csrf_meta_tags %>
    <%= action_cable_meta_tag %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%= stylesheet_link_tag    'application', media: 'all' %>
  </head>
  <body>
    <%= render 'shared/navbar' unless current_page?(landing_path) %>
    <%= render 'shared/flashes' unless current_page?(landing_path) %>
    <%= yield %>
    <%= render 'shared/footer' unless current_page?(landing_path) %>
    <%= javascript_include_tag 'application' %>
  </body>
</html>
